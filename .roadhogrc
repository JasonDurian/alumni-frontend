{
  "entry": "src/index.js",
  "disableCSSModules": false,
  "publicPath": "/dist/",
  "theme": {
    "@primary-color": "#A24824",
    "@link-color": "#A24824"
  },
  "autoprefixer": null,
  "extraBabelPlugins": [
    "transform-runtime",
    ["import", { "libraryName": "antd", "style": true }]
  ],
  "proxy": {
    "/tp": {
      "target": "http://localhost/alumni/",
      "changeOrigin": true,
      "pathRewrite": { "^/tp" : "" }
    }
  },
  "env": {
    "development": {
      "extraBabelPlugins": [
        "dva-hmr"
      ]
    },
    "production": {
    }
  }
}
